<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
<Description about="urn:mozilla:install-manifest">

    <em:id>referrercontrol@qixinglu.com</em:id>
    <em:version>0.3</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>false</em:unpack>
    <em:optionsURL>chrome://referrercontrol/content/options.xul</em:optionsURL>

    <!-- Front End MetaData -->
    <em:name>Referrer Control</em:name>
    <em:description>Control http request referrer</em:description>
    <em:creator>muzuiget</em:creator>

    <!-- Firefox -->
    <em:targetApplication>
    <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>17.0</em:minVersion>
        <em:maxVersion>22.*</em:maxVersion>
    </Description>
    </em:targetApplication>

    <!-- Localize -->
    <em:localized>
    <Description>
        <em:locale>zh-CN</em:locale>
        <em:name>Referrer Control</em:name>
        <em:description>控制 http 协议头的来源地址</em:description>
    </Description>
    <Description>
        <em:locale>zh-TW</em:locale>
        <em:name>Referrer Control</em:name>
        <em:description>控制 http 協議頭的來源地址</em:description>
    </Description>
    </em:localized>

</Description>
</RDF>
